<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	
	<BiomeDef>
		<defName>TemperateGuldenForest</defName>
		<label>temperate Gulden forest</label>
		<description>Forests of Gulden trees interspersed with fertile clearings. Many species of animals move around among the trees and on the plains.</description>
		<workerClass>GuldenBiome.BiomeWorker</workerClass>
		
		<modExtensions>
			<li Class="BiomesKit.BiomesKitControls" MayRequireAnyOf="zal.wmbp,regrowth.botr.core">
				<!--
				No longer required, done through DLL instead
				<allowOnLand>True</allowOnLand>
				<spawnOnBiomes>
					<li>
						<li>TemperateForest</li>
					</li>
				</spawnOnBiomes>
				<minElevation>500</minElevation>
				<maxElevation>800</maxElevation>
				<frequency>100</frequency>
				-->
				
				<forested>true</forested>
				<uniqueHills>true</uniqueHills>
				<forestSparseBelow>1100</forestSparseBelow>
				<forestDenseAbove>1500</forestDenseAbove>
				<!--
				<mountainsSemiSnowyBelow>10</mountainsSemiSnowyBelow>
				<impassableSemiSnowyBelow>25</impassableSemiSnowyBelow>
				<impassableVerySnowyBelow>-5</impassableVerySnowyBelow>
				<mountainsSnowyBelow>-5</mountainsSnowyBelow>
				<impassableSnowyBelow>0</impassableSnowyBelow>
				-->
				<materialRandomRotation>false</materialRandomRotation>
				<smallHillSizeMultiplier>1.5</smallHillSizeMultiplier>
				<largeHillSizeMultiplier>2</largeHillSizeMultiplier>
				<mountainSizeMultiplier>1.4</mountainSizeMultiplier>
				<impassableSizeMultiplier>1.3</impassableSizeMultiplier>
			</li>
		
		</modExtensions>
		<animalDensity>3.7</animalDensity>
		<plantDensity>0.80</plantDensity>
		<settlementSelectionWeight>1</settlementSelectionWeight>
		<movementDifficulty>1</movementDifficulty>
		<texture>World/Biomes/Gulden</texture>
		<forageability>1</forageability>
		<foragedFood>GuldenRawBerries</foragedFood>
		<wildPlantRegrowDays>20</wildPlantRegrowDays>
		<maxFishPopulation>540</maxFishPopulation>
		<fishTypes MayRequire="Ludeon.RimWorld.Odyssey">
			<freshwater_Common>
				<Fish_Salmon MayRequire="Ludeon.RimWorld.Odyssey">1</Fish_Salmon>
				<Fish_Bass MayRequire="Ludeon.RimWorld.Odyssey">1</Fish_Bass>
			</freshwater_Common>
			<freshwater_Uncommon>
				<Fish_Catfish MayRequire="Ludeon.RimWorld.Odyssey">1</Fish_Catfish>
			</freshwater_Uncommon>
			<saltwater_Common>
				<Fish_Salmon MayRequire="Ludeon.RimWorld.Odyssey">1</Fish_Salmon>
				<Fish_Bluefish MayRequire="Ludeon.RimWorld.Odyssey">1</Fish_Bluefish>
			</saltwater_Common>
			<saltwater_Uncommon>
				<Fish_Marlin MayRequire="Ludeon.RimWorld.Odyssey">1</Fish_Marlin>
				<Fish_Flounder MayRequire="Ludeon.RimWorld.Odyssey">1</Fish_Flounder>
			</saltwater_Uncommon>
			<rareCatchesSetMaker MayRequire="Ludeon.RimWorld.Odyssey">RareFishingCatches_Temperate</rareCatchesSetMaker>
		</fishTypes>
		<soundsAmbient>
			<li>Ambient_NightInsects_Standard</li>
		</soundsAmbient>
		<diseaseMtbDays>50</diseaseMtbDays>
		<diseases>
			<li>
				<diseaseInc>Disease_Flu</diseaseInc>
				<commonality>50</commonality>
			</li>
			<li>
				<diseaseInc>Disease_Plague</diseaseInc>
				<commonality>50</commonality>
			</li>
			<li>
				<diseaseInc>Disease_Malaria</diseaseInc>
				<commonality>50</commonality>
			</li>
			<li>
				<diseaseInc>Disease_GutWorms</diseaseInc>
				<commonality>25</commonality>
			</li>
			<li>
				<diseaseInc>Disease_FibrousMechanites</diseaseInc>
				<commonality>15</commonality>
			</li>
			<li>
				<diseaseInc>Disease_SensoryMechanites</diseaseInc>
				<commonality>15</commonality>
			</li>
			<li>
				<diseaseInc>Disease_MuscleParasites</diseaseInc>
